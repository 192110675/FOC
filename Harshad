#include<stdio.h>
int main()
{
//fill the code
int num;
int temp;
printf("Enter the number : ");
scanf("%d",&num);
int sum = 0;
temp = num;
while(temp)
{
sum += temp % 10;
temp = temp / 10;
}
int res = num % sum;
if(res == 0)
printf("Given number is Harshad number");
else
printf("Given number is not Harshad number");
return 0;
}
